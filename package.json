{
  "name": "snext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "//dev:vercel": "NOTE: vercel won't allow you to call this script just 'dev'",
    "dev:vercel": "vercel dev",
    "build": "next build",
    "start": "next start",
    "setup-venv": "python3 -m venv ./venv/ --without-pip",
    "install-python": "pip install -r requirements.txt",
    "save-python-packages": "pip freeze > requirements.txt",
    "postinstall": "yarn run install-python"
  },
  "dependencies": {
    "next": "latest",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "packageManager": "yarn@3.2.2",
  "optionalDependencies": {
    "vercel": "latest"
  }
}
